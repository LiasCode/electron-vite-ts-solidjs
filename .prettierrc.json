{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "useTabs": false,
  "quoteProps": "consistent",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "singleAttributePerLine": true
}
